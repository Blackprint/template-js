	"Blackprint Node with Sketch Interface": {
		"scope": "javascript",
		"prefix": "node",
		"body": [
			"/**",
			" * Node description here",
			" * @summary ${2}",
			" * @blackprint node",
			" */",
			"Blackprint.registerNode(\"LibraryName/${3:Feature/Name}\",",
			"class extends Blackprint.Node {",
			"\tstatic input = {",
			"\t\t${1:PortName: String}",
			"\t};",
			"\tstatic output = {",
			"\t\t${0:PortName: String}",
			"\t};",
			"",
			"\tconstructor(instance){",
			"\t\tsuper(instance);",
			"",
			"\t\tlet iface = this.setInterface(\"BPIC/LibraryName/${3:Feature/Name}\");",
			"\t\tiface.title = \"${2}\";",
			"\t}",
			"});",
			"",
			"Blackprint.registerInterface(\"BPIC/LibraryName/${3:Feature/Name}\",",
			"Context.IFace[\"${3:Feature/Name}\"] = class extends Blackprint.Interface {",
			"// Make sure to save it to the Context, so we can access it from .sf extension",
			"",
			"\tconstructor(node){",
			"\t\tsuper(node);",
			"\t}",
